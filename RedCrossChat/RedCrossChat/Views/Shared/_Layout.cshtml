<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title> @Constants.Sitename | @ViewData["Title"]</title>

    <environment names="Development,Staging,Production">
        <!-- Favicon -->
        <link rel="shortcut icon" href="~/img/favicon.png" type="image/x-icon">

        <!-- Add local styles, mostly for plugins css file -->
        @if (IsSectionDefined("Styles"))
        {
            @RenderSection("Styles", required: false) 
        }

        @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
        <link rel="stylesheet" href="~/css/site.css" />

        <!-- Google Font: Source Sans Pro -->
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
        <!-- Font Awesome -->
        <link rel="stylesheet" href="~/lib/fontawesome-free/css/all.min.css">
        <!-- Ionicons -->
        <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
        <!-- Tempusdominus Bootstrap 4 -->
        <link rel="stylesheet" href="~/lib/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
        <!-- SweetAlert2 -->
        <link rel="stylesheet" href="~/lib/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css">
        <!-- Select2 -->
        <link rel="stylesheet" href="~/lib/select2/css/select2.min.css">
        <link rel="stylesheet" href="~/lib/select2-bootstrap4-theme/select2-bootstrap4.min.css">
        <!-- BS Stepper -->
        <link rel="stylesheet" href="~/lib/bs-stepper/css/bs-stepper.min.css">
        <!-- Toastr -->
        <link rel="stylesheet" href="~/lib/toastr/toastr.min.css">
        <!-- iCheck bootstrap
        <link rel="stylesheet" href="~/lib/icheck-bootstrap/icheck-bootstrap.min.css"> -->
        <!-- icheck -->
        <link rel="stylesheet" href="~/lib/iCheck/skins/minimal/_all.css">
        <link rel="stylesheet" href="~/lib/iCheck/custom.css">
        <!-- JQVMap -->
        @*<link rel="stylesheet" href="~/lib/jqvmap/jqvmap.min.css">*@
        <!-- Theme style -->
        <link rel="stylesheet" href="~/adminlte/css/adminlte.min.css">
        <!-- overlayScrollbars -->
        <link rel="stylesheet" href="~/lib/overlayScrollbars/css/OverlayScrollbars.min.css">
        <!-- Daterange picker -->
        <link rel="stylesheet" href="~/lib/daterangepicker/daterangepicker.css">
        <!-- summernote -->
        <link rel="stylesheet" href="~/lib/summernote/summernote-bs4.min.css">
    </environment>
</head>
<body hold-transition sidebar-mini>
    <!-- Wrapper-->
    <div class="wrapper"> @*id="wrapper" class="@Html.PageClass()>*@

        <partial name="_Navbar" />
        <partial name="_Sidebar" />

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>

        <partial name="_Footer" />
    </div>    

    <environment names="Development,Staging,Production">
        @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@

        <!-- jQuery -->
        <script src="~/lib/jquery/jquery.min.js"></script>
        <!-- jQuery UI 1.11.4 -->
        <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
        <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
        <script>
          $.widget.bridge('uibutton', $.ui.button)
        </script>
        <!-- Bootstrap 4 -->
        <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
        <!-- ChartJS -->
        <script src="~/lib/chart.js/Chart.min.js"></script>
        <!-- Sparkline -->
        <script src="~/lib/sparklines/sparkline.js"></script>
        <!-- JQVMap -->
       @* <script src="~/lib/jqvmap/jquery.vmap.min.js"></script>
        <script src="~/lib/jqvmap/maps/jquery.vmap.usa.js"></script>*@
        <!-- jQuery Knob Chart -->
        @*<script src="~/lib/jquery-knob/jquery.knob.min.js"></script>*@
        <!-- daterangepicker -->
        <script src="~/lib/moment/moment.min.js"></script>
        <script src="~/lib/daterangepicker/daterangepicker.js"></script>
        <!-- Tempusdominus Bootstrap 4 -->
        <script src="~/lib/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
        <!-- SweetAlert2 -->
        <script src="~/lib/sweetalert2/sweetalert2.min.js"></script>
        <!-- Select2 -->
        <script src="~/lib/select2/js/select2.full.min.js"></script>
        <!-- BS-Stepper -->
        <script src="~/lib/bs-stepper/js/bs-stepper.min.js"></script>
        <!-- Toastr -->
        <script src="~/lib/toastr/toastr.min.js"></script>
        <!-- icheck -->
        <script src="~/lib/iCheck/icheck.min.js"></script>
        <!-- Summernote -->
        <script src="~/lib/summernote/summernote-bs4.min.js"></script>
        <!-- overlayScrollbars -->
        <script src="~/lib/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
        <!-- AdminLTE App -->
        <script src="~/adminlte/js/adminlte.min.js"></script>

        <script src="~/js/util.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @*<script src="~/js/control_sidebar.js" asp-append-version="true"></script>*@
    </environment>


    @RenderSection("Scripts", required: false)
</body>
</html>
